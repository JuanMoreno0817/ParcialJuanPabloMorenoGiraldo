@model ParcialServicios.DAL.Entities.Ticket
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="UseDate" />
            <input type="hidden" asp-for="IsUsed" />
            <div class="form-group">
                <label asp-for="EntranceGate" class="control-label">Porteria</label>
                <input asp-for="EntranceGate" class="form-control" />
                <span asp-validation-for="EntranceGate" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Actualizar" class="btn btn-outline-primary" />
                <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
            </div>
        </form>
    </div>
</div>